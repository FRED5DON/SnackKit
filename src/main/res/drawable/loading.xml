<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">



    <item>
        <shape
            android:innerRadius="2dp"
            android:shape="oval">
            <stroke android:color="@color/colorPrimaryDark" />
            <solid android:color="@color/colorPrimaryDark" />
            <size
                android:width="126dp"
                android:height="126dp" />
        </shape>

    </item>

    <item>
        <shape
            android:innerRadius="2dp"
            android:shape="oval">
            <stroke android:color="@color/colorAccentAlpha" />
            <solid android:color="@color/colorAccentAlpha" />
            <size
                android:width="6dp"
                android:height="6dp" />
        </shape>

    </item>


</layer-list>